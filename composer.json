{
    "name": "orobogenius/laravel-model-statable",
    "license": "MIT",
    "description": "Add states to your laravel models.",
    "keywords": [
        "laravel",
        "eloquent",
        "states",
        "statable",
        "laravel-statable"
    ],
    "homepage": "https://github.com/orobogenius/laravel-model-statable",
    "authors": [{
        "name": "Lucky Ozoka",
        "email": "orobolucky@gmail.com"
    }],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Orobogenius\\Statable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orobogenius\\Statable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}